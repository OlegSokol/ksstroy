<%@ tag body-content="empty" trimDirectiveWhitespaces="true" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ attribute name="innerMaterialTypeData" required="true" type="ua.ksstroy.logic.material.MaterialTypeData" %><%@ taglib prefix="material" tagdir="/WEB-INF/tags/material" %><spring:url value="/resources/img/done_btn.png" var="done_btn"/><div class="controller hidden">    <div class="controller_name">Add Material Type</div>    <form action="${pageContext.request.contextPath}/projects/addMaterialType" method="post">        <input type="hidden" name="parentId" value="${innerMaterialTypeData.id}">        <input type="hidden" name="projectId" value="${projectId}">        <table>            <tbody>            <tr>                <td>name:</td>                <td><input type="text" name="name" placeholder="Enter name"></td>            </tr>            <tr>                <td>description:</td>                <td><input type="text" name="description" placeholder="Enter description"></td>            </tr>            </tbody>        </table>        <input class="accept_changes_btn clickableIcon" type="image" src="${done_btn}">    </form></div>