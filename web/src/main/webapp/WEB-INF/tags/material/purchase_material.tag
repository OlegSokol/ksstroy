<%@tag pageEncoding="UTF-8"%><%@ tag body-content="empty" trimDirectiveWhitespaces="true" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ attribute name="materialTypeData" required="true" type="ua.ksstroy.logic.material.MaterialTypeData" %><spring:url value="/resources/img/done_btn.png" var="done_btn"/><div class="controller hidden item">    <div class="controller_name">By Material</div>    <form action="${pageContext.request.contextPath}/projects/purchaseMaterial" method="post">        <input type="hidden" name="parentId" value="${materialTypeData.id}">        <input type="hidden" name="projectId" value="${projectId}">        <table>            <tbody>            <tr>                <td>Quantity:</td>                <td><input type="text" name="quantity" placeholder="Enter quantity"></td>            </tr>            <tr>                <td>PricePerUnit:</td>                <td><input type="text" name="price" value="${materialTypeData.pricePerUnit}" disabled></td>            </tr>            </tbody>        </table>        <input class="accept_changes_btn clickableIcon" type="image" src="${done_btn}">    </form></div>