<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ attribute name="workTypeGroup" required="true" type="ua.ksstroy.logic.worktype.WorkTypeGroupData" %><%@ taglib prefix="worktype" tagdir="/WEB-INF/tags/worktype" %><spring:url value="/resources/img/arrow_drop_down.png" var="arrow_drop_down"/><spring:url value="/resources/img/add_btn.png" var="add_btn"/><spring:url value="/resources/img/delete_btn.png" var="delete_btn"/><spring:url value="/resources/img/show_updates_btn.png" var="show_update_forms_btn"/><c:if test="${workTypeGroup.name ne 'root_worktype_group'}">    <div class="item">        <img class="dropdown_btn clickableIcon" src="${arrow_drop_down}" alt="Arrow">            ${workTypeGroup.name}        <div class="add_delete_wrapper">            <form action="${pageContext.request.contextPath}/workType/removeWorkTypeGroup" method="post">                <input type="hidden" name="id" value="${workTypeGroup.id}">                <input type="hidden" name="projectId" value="${projectId}">                <input class="delete_btn clickableIcon" type="image" src="${delete_btn}" alt="Delete">            </form>            <img class="add_btn clickableIcon" src="${add_btn}" alt="Add">            <img class="show_update_forms_btn clickableIcon" src="${show_update_forms_btn}" alt="show_update_forms">        </div>        <worktype:addWorkTypes workTypeGroup="${workTypeGroup}"/>        <worktype:addWorkTypeGroup innerWorkTypeGroup="${workTypeGroup}"/>        <worktype:updateWorkTypeGroup workTypeGroup="${workTypeGroup}"/>    </div></c:if>